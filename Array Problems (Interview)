#include <iostream>
#include<climits>
using namespace std;

//Find MIN and MAX
void Max_MinElement()
{
  int arr[]={4,56,-6,12,0,230};
  int max=INT_MIN,min=INT_MAX;
  int n=sizeof(arr)/sizeof(arr[0]);
  
  for(int i=0 ; i<n ;i++){
    if(arr[i]>max)
      max=arr[i];
      
    if(arr[i]<min)
      min=arr[i];
  }
  cout<<"Maximum Element:- "<<max<<"\n";
  cout<<"Minimum Element:- "<<min;
}


//Find Unique(long method)
int findUniqueElement()
{
  int arr[]={4,9,3,9,1},i,j;
  int size=sizeof(arr)/sizeof(arr[0]);
  int unique;
  
  for(i=0 ; i<size ; i++)
  {
    unique=arr[i];
    for(j=0; j<size ; j++)
    {
     if(unique==arr[j] && i!=j)
       break;
    } 
    if(j==size)
      return unique;
  }return -1;
}

//Find Unique using XOR ^
int findUniqueUsingXOR()
{
 int arr[]={4,9,3,9,4};
 int size=sizeof(arr)/sizeof(arr[0]);
 int uniXor;
 for(int i=0 ; i<size; i++)
   uniXor=uniXor^arr[i];
  return uniXor;
}


void main()
{
 Max_MinElement();
 cout<<findUniqueElement();
 cout<<findUniqueUsingXOR();
 }

